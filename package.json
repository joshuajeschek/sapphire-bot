  "license": "MIT",
  "repository": "github:/joshuajeschek/sapphire-bot",
  "homepage": "https://www.github.com/joshuajeschek/sapphire-bot",
  "bugs": "https://github.com/joshuajeschek/sapphire-bot/issues",
  "dependencies": {
    "@discordjs/collection": "^0.6.0",
    "@prisma/client": "^3.14.0",
    "@sapphire/decorators": "^4.3.4",
    "@sapphire/discord-utilities": "^2.10.3",
    "@sapphire/discord.js-utilities": "^4.10.0",
    "@sapphire/fetch": "^2.3.1",
    "@sapphire/framework": "next",
    "@sapphire/plugin-editable-commands": "^1.2.0",
    "@sapphire/plugin-logger": "^2.2.1",
    "@sapphire/stopwatch": "^1.4.1",
    "@sapphire/time-utilities": "^1.7.4",
    "@sapphire/type": "^2.2.4",
    "@sapphire/utilities": "^3.6.2",
    "discord.js": "^13.6.0",
    "dotenv-cra": "^3.0.2",
    "lodash": "^4.17.21",
    "node-vibrant": "^3.2.1-alpha.1",
    "reflect-metadata": "^0.1.13",
    "systeminformation": "^5.11.15"
  },
  "devDependencies": {
    "@sapphire/prettier-config": "^1.4.3",
    "@sapphire/ts-config": "^3.3.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.32",
    "@types/ws": "^8.5.3",
    "dotenv-cli": "^5.1.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc --build src",
    "watch": "tsc --build src -w",
    "start": "node dist/index.js",
    "dev": "run-s build hmr",
    "hmr": "run-p watch start",
    "prod": "NODE_ENV=production run-s build start",
    "watch:start": "tsc-watch --build src --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:dev": "PRISMA_SCHEMA=dev dotenv -e src/prisma.env npx prisma",
    "prisma:prod": "PRISMA_SCHEMA=prod dotenv -e src/prisma.env npx prisma"
  },
  "prettier": "@sapphire/prettier-config"
}
